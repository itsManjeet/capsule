let f0 = fun () { return 0; };
println (f0 ());

let f1 = fun (a) { return a; };
println (f1 (1));

let f2 = fun (a, b) { return a + b; };
println (f2 (1, 2));

let f3 = fun (a, b, c) { return a + b + c; };
println (f3 (1, 2, 3));

let f4 = fun (a, b, c, d) { return a + b + c + d; };
println (f4 (1, 2, 3, 4));

let f5 = fun (a, b, c, d, e) { return a + b + c + d + e; };
println (f5 (1, 2, 3, 4, 5));

let f6 = fun (a, b, c, d, e, f) { return a + b + c + d + e + f; };
println (f6 (1, 2, 3, 4, 5, 6));

let f7 = fun (a, b, c, d, e, f, g) { return a + b + c + d + e + f + g; };
println (f7 (1, 2, 3, 4, 5, 6, 7));

let f8 = fun (a, b, c, d, e, f, g, h) { return a + b + c + d + e + f + g + h; };
println (f8 (1, 2, 3, 4, 5, 6, 7, 8));

let f9 = fun (a, b, c, d, e, f, g, h, i) { return a + b + c + d + e + f + g + h + i; };
println (f9 (1, 2, 3, 4, 5, 6, 7, 8, 9));